extends layout

block content
  h1= title
  p Welcome to #{title}
  a(href='/inventory/products') Check all products

  div.countDiv 
    div.prodDiv
      div.divCount=prodCount    
      div.divTitle Products
    div.brandDiv
      div.divCount=brandCount    
      div.divTitle Brands
    div.catCount
      div.divCount=catCount    
      div.divTitle Categories